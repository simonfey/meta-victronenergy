Subject: [PATCH 1/9] Kconfig: venus: select the mediatek wifi modules

---
 Kconfig.venus | 6 ++++++
 1 file changed, 6 insertions(+)

diff --git a/Kconfig.venus b/Kconfig.venus
index 2407d0f83a89..dee960bbd1b9 100644
--- a/Kconfig.venus
+++ b/Kconfig.venus
@@ -248,6 +248,12 @@ config VENUS_MODULES
 	select RTL8187
 	select RTL_CARDS
 	select RTL8192CU
+	select WLAN_VENDOR_MEDIATEK
+	select MT7601U
+	select MT76x0U
+	select MT76x2U
+	select MT7663U
+	select MT7663S
 
 	# ppp
 	select PPP
